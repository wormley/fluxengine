comment: 'Acorn DFS 100kB/200kB 3.5" or 5.25" 40- or 80-track SS (ro)'

image_writer {
	filename: "acorndfs.img"
	img {
		trackdata {
			sector_range {
				start_sector: 0
			}
		}
	}
}

decoder {
	ibm {
		trackdata {
			ignore_side_byte: true
		}
	}
}

tracks {
	start: 0
	end: 79
}

heads {
	start: 0
	end: 0
}


